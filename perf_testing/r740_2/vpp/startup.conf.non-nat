unix {
  startup-config startup.cfg
  nodaemon
  cli-no-banner
  cli-no-pager
  cli-prompt srv-2-vpp#
  cli-listen /run/vpp/cli.sock
  log vpp.log
  full-coredump
}

cpu {
  main-core 2
  corelist-workers 4,6
}

dpdk {
  socket-mem 2048,0
  log-level debug
  no-tx-checksum-offload
  dev default{
    num-tx-desc 512
    num-rx-desc 512
    # num-rx-queues 10
  }
  dev 0000:1b:00.0 {
    # pf of eno3
    workers 0
    name eth0
  }
  dev 0000:19:10.0 {
    # one vf of eno1: enp25s16
    workers 1
    name eth1
  }
  no-multi-seg
}

ip {
  heap-size 4G
}

statseg {
  size 4G
}
